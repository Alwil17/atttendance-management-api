{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run vercel-build",
  "devCommand": "npm run dev"
}